{
    "name": "ai-stream-agent",
    "version": "1.0.0",
    "description": "AI streaming agent server built with Node.js, Express, and TypeScript that interacts with the OpenAI API to provide real-time chat responses",
    "author": "Ignacio Villamar",
    "license": "ISC",
    "type": "commonjs",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivstudio/ai-stream-agent.git"
    },
    "keywords": [
        "express",
        "chat",
        "typescript",
        "openai"
    ],
    "main": "dist/app.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "start": "node dist/app.js",
        "lint": "eslint 'src/**/*.ts' --fix",
        "format": "prettier --write 'src/**/*.ts'"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "openai": "^4.85.3",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/express-rate-limit": "^5.1.3",
        "@typescript-eslint/eslint-plugin": "^8.24.1",
        "@typescript-eslint/parser": "^8.24.1",
        "eslint": "^9.21.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.3",
        "prettier": "^3.5.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.7.3"
    }
}
